<div id="wrapper">
                <div class="content" style="margin-top: 5px">
                    <div class="container col-md-offset-3 col-md-6">
                        <h2 id="bwtitle" style="text-align: center;">Importer un nouveau fichier</h2>
                        <form enctype="multipart/form-data" action="import_xls" method="post" class="form-horizontal">
                            <div class="form-group">
                                <label class="control-label col-md-4" for="multi-append">Fichier</label>
                                <div class="col-md-8">
                                    <input type="file" id="file" name="file" required="true"/>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-4" for="multi-append">Date de l'import</label>
                                <div class="col-md-8">
                                    <input name="date-import" id='date-import' type='date' value="" class='form-control'/>
                                </div>
                            </div>

                            <button type="submit" class="btn btn-default" style="text-align: center; margin: 0 50%;">Importer</button>
                        </form>
                    </div>
                </div>
            </div>